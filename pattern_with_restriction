#include <stdio.h>

int main()
{
    int num;
    scanf("%d",&num);
    int i,j;
    int count=1;
    for(i=1;i<=num;i++){
        for(j=1;j<=i;j++){
            if(count>num){
                break;
            }
            printf("%d",count);
            count++;
        }
        printf("\n");
    }

    return 0;
}
